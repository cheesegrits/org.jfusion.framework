{
    "name": "jfusion/framework",
    "type": "library",
    "description": "JFusion Framework, used to create user integration and DSLO",
    "keywords": ["jfusion", "framework"],
    "homepage": "http://jfusion.org",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Morten Hundevad",
            "email": "fannoj@gmail.com",
            "homepage": "http://fanno.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.10",
        "jfusion/debugger": "~1.*@stable",
        "jfusion/curl": "~1.*@stable",
        "joomla/archive": "~1.*@stable",
        "joomla/database": "~1.*@stable",
        "joomla/date": "~1.*@stable",
        "joomla/event": "~1.*@stable",
        "joomla/filesystem": "~1.*@stable",
        "joomla/filter": "~1.*@stable",
        "joomla/form": "~1.*@stable",
        "joomla/input": "~~1.*@stable",
        "joomla/language": "~1.*@stable",
        "joomla/registry": "~1.*@stable",
        "joomla/string": "~1.*@stable",
        "joomla/uri": "~1.*@stable",
        "joomla/utilities": "~1.*@stable",
        "joomla/crypt": "~1.*@stable",
        "symfony/yaml": "~2.*@stable"
    },
    "autoload": {
        "psr-4": {
            "JFusion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JFusion\\Tests\\": "Tests/",
            "JFusion\\Plugins\\mockplugin\\": "Tests/Mock/Mockplugin/"
        }
    },
    "require-dev": {
        "jfusion/platform-joomla": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.3.*"
    }
}